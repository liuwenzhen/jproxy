<script>
	(function($){
		$(document).ready(function(){
			$(".sw > .tab").hover(function(){
				var thiz = $(this);
				var sibs = thiz.siblings("li");
				sibs.each(function(){
					var sib = $(this);
					sib.removeClass("on");
					var ref = sib.attr("ref");
					$("."+ref).hide();
				});
				var ref = thiz.attr("ref");
				thiz.addClass("on");
				$("."+ref).show();
			},function(){
					
			});
		});
	}(jQuery));
</script>
