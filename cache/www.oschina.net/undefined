		<script type="text/javascript">
			function hbTab(n) {
				for (var i = 1; i <= 2; i++) {
					if (i == n) {
						getObject("hbTab" + i).className = "ad_Tabopen";
						getObject("hbTable" + i).style.display = "";
					} else {
						getObject("hbTab" + i).className = "ad_Tabclose";
						getObject("hbTable" + i).style.display = "none";
					}
				}
			}
		</script>
		<script type="text/javascript">
		try{
		function initTabs(){
		var vd=new Cookie(document,"SOHUHOMETAB",24);
		vd.load();
		vd.visit=(vd.visit==null)?parseInt(Math.random()*2+1):vd.visit;
		if(vd.visit!=0)hbTab(vd.visit);
		vd.visit++;
		vd.visit=(vd.visit>2)?1:vd.visit;
		vd.store();
		}
		initTabs();
		}catch(e){}
		</script></div>
