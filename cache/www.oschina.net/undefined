		<script type="text/javascript">
            function focusTab(n) {
                for (var i = 1; i <= 4; i++) {
                    if (i == n) {
                        getObject("focusTab" + i).className = "ad_Tabopen";
                        getObject("focusTable" + i).style.display = "";
                    } else {
                        getObject("focusTab" + i).className = "ad_Tabclose";
                        getObject("focusTable" + i).style.display = "none";
                    }
                }
            }

    </script></SOHUADCODE>
