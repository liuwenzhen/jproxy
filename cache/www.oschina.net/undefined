<script type="text/javascript">
	(function(){

		var startPic = getStartPos(),
			startOK = false;
		function getStartPos(){
			var hash = document.location.hash;
				hash = hash.substring(1);
			if(/img\d{1}/g.test(hash)){
				return /img(\d)/g.exec(hash)[1];
				//return hash.substring(3,4);
			}
		}
		function picRolling(){	
			var getUl=document.getElementById('pic-cont'),getLi=getUl.getElementsByTagName

('li'),getImg=getUl.getElementsByTagName('img'),	
			getRight=document.getElementById('right-btn'),getLeft=document.getElementById('left-btn'),	
			timer=null, //��������ļ�ʱ��	
			timer_one = null, //����һ��ͼ�ļ�ʱ��	
			shifting = 10, //������λ,һ�ι�������	
			speed = 5, //ѭ��������Сʱ����	
			timer_out = null, //��������е�ͣ��	
			ifMouseOver = false, //��־λ,����Ƿ���ͣ	
			imgWidth = 190, //ͼƬ���	
			temp = 0,	
			newUlNode = document.createElement('ul'); //Ϊ��ͼƬ���޷����,������ͼƬ�����ٸ���һ��ͼƬ;	
			getUl.appendChild(newUlNode);	
			newUlNode.innerHTML = getUl.getElementsByTagName('ul')[0].innerHTML;	
			getUl.style.width = (getLi[0].offsetWidth)*getLi.length+'px'; //���ÿ���ʾ�Ŀ�ȷ�Χ	
			var ulWidth = getUl.offsetWidth;	
			if(startPic){
				var leftPicPos = -imgWidth*(startPic-1);
				getUl.style.left = leftPicPos+'px';
				temp = leftPicPos;
				setTimeout(function(){startOK = true},15000);
			}else{
				startOK = true;
			}
			timer_out = window.setTimeout(function(){timer=setInterval(roll,speed)},3000);	
			getRight.onclick=function(){ //���Ұ�ť��,����һ��ͼƬ	
				if(temp % imgWidth == 0){ //��֤������һ����ͼ,Ҳ������ֻ�����˰���ͼ�������,��������ť	
					theShifting = 0 - shifting;	
					clearInterval(timer);	
					clearTimeout(timer_out);	
					timer_one = setInterval(rollOneTime,speed);	
				}		
				return false;	
			}	
			getRight.onmouseover = function(){	
				getRight.getElementsByTagName("a")[0].className = "now-btn";	
				getLeft.getElementsByTagName("a")[0].className = "now-btn";	
			}	
			getLeft.onmouseover = function(){	
				getRight.getElementsByTagName("a")[0].className = "now-btn";	
				getLeft.getElementsByTagName("a")[0].className = "now-btn";	
			}	
			getRight.onmouseout = function(){	
				getRight.getElementsByTagName("a")[0].className = "";	
				getLeft.getElementsByTagName("a")[0].className = "";	
			}	
			getLeft.onmouseout = function(){	
				getRight.getElementsByTagName("a")[0].className = "";	
				getLeft.getElementsByTagName("a")[0].className = "";	
			}	
			getLeft.onclick=function(){ //����ť��,����һ��ͼƬ	
				if(temp % imgWidth == 0){ //��֤������һ����ͼ,Ҳ������ֻ�����˰���ͼ�������,��������ť	
					theShifting = shifting;	
					clearInterval(timer);	
					clearTimeout(timer_out);	
					timer_one = setInterval(rollOneTime,speed);	
				}		
				return false;	
			}
			function rollOneTime(){  //��ͼֻ����һ��ͼƬ�ľ���,Ҳ����ֻ����һ��ͼƬ	
				clearInterval(timer);	
				clearTimeout(timer_out);	
				getUl.style.left = temp + theShifting + 'px';  //ÿ��Ų��theShifting������,temp��¼��offsetLeftֵ	
				temp = temp + theShifting; //temp��¼��offsetLeftֵ	
				if(temp % imgWidth == 0){ //��֤������һ����ͼ	
					clearInterval(timer_one);	
					clearInterval(timer);	
					timer_out = window.setTimeout(function(){timer=setInterval(roll,speed)},3000); //��֤ͼչʾ3�����ִ��roll	
				}
				if(temp < -ulWidth/2){ //temp��¼��offsetLeftֵ,������ͼ����Ų����ʱ,��ͼ���¶�λleftֵΪ0	
					getUl.style.left=0;	
					temp = 0;	
				}	
				if(temp > 0){ //������ͼ���Ҷ�Ų����ʱ,��ͼ���¶�λleftֵ	
					getUl.style.left = -ulWidth/2+'px';	
					temp = -ulWidth/2;	
				}		
			}	
			function roll(){  //��ͼѭ������	
				if(!startOK){return};
				clearInterval(timer_one);	
				clearTimeout(timer_out);	
				theShifting = 0 - shifting;  //Ĭ����ͼ�������	
				getUl.style.left = temp + theShifting+'px'; //ÿ��Ų��theShifting������,temp��¼��offsetLeftֵ	
				temp = temp + theShifting;  //temp��¼��offsetLeftֵ	
				if(temp % imgWidth == 0){ //��֤������һ����ͼ	
					clearInterval(timer);	
					if(!ifMouseOver){	
						timer_out = window.setTimeout(function(){timer=setInterval(roll,speed)},3000); //��֤ͼչʾ3�����ִ��roll	
					}	
				}	
				if(temp<-ulWidth/2){  //temp��¼��offsetLeftֵ,������ͼ����Ų����ʱ,��ͼ���¶�λleftֵΪ0	
					getUl.style.left=0;	
					temp = 0;	
				}	
				if(temp>0){ //������ͼ���Ҷ�Ų����ʱ,��ͼ���¶�λleftֵ	
					getUl.style.left=-ulWidth/2+'px';	
					temp = -ulWidth/2;	
				}	
			}									
			for(var i=0; i<getLi.length; i++){   //����ÿ��ͼ,��ÿ��ͼ�ϰ�mouseover,mouseout�¼�	
				getLi[i].onmouseover=function(){ //�����ͣ��ͼƬ��ʱȡ�����	
					if(getUl.offsetLeft % imgWidth == 0){  //��֤������һ����ͼ��ȡ�����,�������ͣ���ڰ���ͼ״̬	
						clearInterval(timer);	
						clearTimeout(timer_out);	
						getRight.getElementsByTagName("a")[0].className = "now-btn";	
						getLeft.getElementsByTagName("a")[0].className = "now-btn";	
					}	
				}	
				getLi[i].onmouseout=function(){ //���Ų��ͼƬ�������	
					if(getUl.offsetLeft % imgWidth == 0){	
						clearInterval(timer);	
						clearTimeout(timer_out);	
						getRight.getElementsByTagName("a")[0].className = "";	
						getLeft.getElementsByTagName("a")[0].className = "";	
						timer_out = window.setTimeout(function(){timer=setInterval(roll,speed)},3000);	
					}	
				}	
			}	
		}	
		//window.onload = function(){picRolling();}	
		if(window.attachEvent){	
			window.attachEvent("onload", picRolling);	
		}else{	
			window.addEventListener("load", picRolling , false);	
		}	
	})()	
</script>
