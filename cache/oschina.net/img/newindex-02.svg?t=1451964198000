            <script type='text/javascript'>
                $(".TweetsSwitcher").ready(function () {
                    //预加载切换后的两张图片
                    var img4 = new Image();
                    img4.src = '/img/tweetsSwitcher4.gif?t=1451964198000';
                    var img3 = new Image();
                    img3.src = '/img/tweetsSwitcher3.gif?t=1451964198000';
                    $(".TweetsSwitcher .sw").click(function () {
                        if ($(this).attr("id") == "newSwitcher") {
                            $(".HotTweets").hide();
                            $(".TopTweets").show();
                        } else {
                            $(".HotTweets").show();
                            $(".TopTweets").hide();
                        }
                        $(".TweetsSwitcher .sw[selected='selected']").removeAttr("selected");
                        $(this).attr("selected", "selected");
                        switchTweets($(this));
                    });
                });
                function switchTweets(t, type) {
                    $(".TweetsSwitcher li").removeClass("on");
                    t.addClass("on");
                    if (t.attr("id") == "newSwitcher") {
                        $("#imgNew").removeClass("img_new_off");
                        $("#imgNew").addClass("img_new_on");
                        $("#imgTop").removeClass("img_top_on");
                        $("#imgTop").addClass("img_top_off");
                    } else {
                        $("#imgNew").removeClass("img_new_on");
                        $("#imgNew").addClass("img_new_off");
                        $("#imgTop").removeClass("img_top_off");
                        $("#imgTop").addClass("img_top_on");
                    }
                }

            </script>
