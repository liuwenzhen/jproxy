                $('#TForm').ajaxForm({
                    dataType: 'json',
                    success: function (json) {
                        handleLogRes(json);
                    }
                });
                
                function submitMe() {
                    var queryString = $('#TForm').formSerialize();
                    ajax_post("/action/tweet/pub", queryString, function (res) {
                        var json = eval('(' + res + ')');
                        handleLogRes(json);
                    });
                }
                function handleLogRes(json) {
                    if (json.msg) {
                        $('#tweet_error_msg').html(json.msg);
                        $('#tweet_error_msg').show();
                    } else if (json.log) {
                        $('#tweet_error_msg').html('');
                        $('#tweet_error_msg').hide();
                        $('#TForm textarea').val(' ');
                        $('#DivPubStatus').show();
                        t = setTimeout(function () {
                            $('#DivPubStatus').hide();
                            clearTimeout(t);
                            $('#TForm textarea').val('');
                            $('#TForm textarea').focus();
                            check_new_log();
                        }, 1000);
